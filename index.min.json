{
  "name": "Tachiyomi: DragonBallAF",
  "pkg": "eu.kanade.tachiyomi.extension.all.dragonballaf",
  "apk": "tachiyomi-all.dragonballaf-v1.0.0.apk",
  "lang": "pt",
  "code": 1,
  "version": "1.0.0",
  "nsfw": 0,
  "sources": [
    {
      "name": "DragonBallAF",
      "lang": "pt",
      "id": "dragonballaf123",
      "baseUrl": "https://dragonballaf100.blogspot.com"
    }
  ],
  "chapters": {
    "list": {
      "selector": "div.chapter", // CSS Selector para os capítulos
      "title": "h3.title a", // Selector para o título do capítulo
      "url": "h3.title a", // Selector para a URL do capítulo
      "date": "span.date", // Selector para a data de publicação (opcional)
      "next": "a.next" // Selector para a próxima página, se houver
    },
    "details": {
      "selector": "div.chapter-content", // CSS Selector para o conteúdo do capítulo
      "image": "img.chapter-image", // Selector para as imagens do capítulo
      "next": "a.next-chapter" // Selector para o próximo capítulo, se houver
    }
  }
}